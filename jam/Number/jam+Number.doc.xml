<?xml-stylesheet href="../-mix+doc/index.xsl" type="text/xsl" ?>
<doc:root
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:wc="https://github.com/nin-jin/wc"
    xmlns:doc="https://github.com/nin-jin/doc"
    >

    <h1>$jam.$Number</h1>
    <p>Создаёт обёртку для чисел.</p>

    <wc:test-js> _done( $jam.$Number ) </wc:test-js>
    <wc:test-js>
        _done( $jam.$Number( 123 ) )
    </wc:test-js>
    <wc:test-js>
        var numb= $jam.$Number( 123 )
        _done( $jam.$Number( numb ) !== numb )
    </wc:test-js>
    <wc:test-js>
        var numb= $jam.$Number( 123 )
        _done( numb.$, 123 )
    </wc:test-js>
    <wc:test-js>
        var numb= $jam.$Number( 123 )
        _done( +numb, 123 )
    </wc:test-js>

</doc:root>
